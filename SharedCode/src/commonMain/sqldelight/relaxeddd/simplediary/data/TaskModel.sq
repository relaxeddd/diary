
CREATE TABLE taskModel (
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    title TEXT NOT NULL,
    desc TEXT
);

selectAll:
SELECT * FROM taskModel;

lastInsertRowId:
SELECT last_insert_rowid();

insertItem:
INSERT OR FAIL INTO taskModel(title, desc) VALUES (?, ?);

deleteItem:
DELETE FROM taskModel WHERE id = ?;

updateItem:
REPLACE INTO taskModel(id, title, desc) VALUES (?, ?, ?);